#!/bin/csh -f
#set dst = (-50 -100 -150 -200)
set ut = (0 3 6 9 12 15 18 21)
set TU = (00 03 06 09 12 15 18 21)

foreach year (2015 2010 2005 2000 1995 1990 1985 1980 1975 1970 1965 1960 1955)
#mkdir $year
cd $year
echo $year
set i = 0

while ($i < "11") 
    @ j =  $i + 1
    set l = 1
    while ($l < "9")
	../premap $year $i $TU[$l] WorldMapKp$i-Ut$ut[$l].out 
	@ l = $l + 1
    end
    @ i = $i + 1
end

cd ..
end
